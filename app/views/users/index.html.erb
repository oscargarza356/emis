<% provide(:title, 'All Users') %>

<h1>All Users</h1>

<div class="bigbox" id="user_display">
  <div class="box">
    <table class="table-minimal">
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <%= render user %>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
